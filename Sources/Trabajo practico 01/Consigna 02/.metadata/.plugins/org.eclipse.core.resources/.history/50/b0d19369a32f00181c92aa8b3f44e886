}
    string getApellido() {
		return apellido;
	}

	void setApellido(string apellido) {
		this->apellido = apellido;
	}

	long getDni() {
		return dni;
	}

	void setDni(long dni) {
		this->dni = dni;
	}

	 string getNombre() {
		return nombre;
	}

	void setNombre(string nombre) {
		this->nombre = nombre;
	}

