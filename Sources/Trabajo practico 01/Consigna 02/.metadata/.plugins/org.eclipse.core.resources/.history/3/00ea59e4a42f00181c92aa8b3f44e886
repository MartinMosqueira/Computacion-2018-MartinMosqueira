    string Persona::getnombre() {
		return apellido;
	}

	void Persona::setapellido(string apellido) {
		this->apellido = apellido;
	}

	long Persona::getdni() {
		return dni;
	}

	void Persona::setdni(long dni) {
		this->dni = dni;
	}

	 string Persona::getnombre() {
		return nombre;
	}

	void Persona::setnombre(string nombre) {
		this->nombre = nombre;
	}

